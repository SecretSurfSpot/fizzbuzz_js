function FizzBuzz(number) {
  if ((number % 3 === 0) && (number % 5 === 0)) {
    //console.log("Fizzbuzz");
    return "FizzBuzz";
    //debugger;
  }
  else if (number % 3 === 0) {
    //console.log("Fizz");
    return "Fizz";
  }
  else if (number % 5 == 0) {
    //console.log("Buzz");
    return "Buzz";
  };
};

for (var number = 1; number <101; number++) {
    console.log("number: " + number + " returns: " + FizzBuzz(number));
}
